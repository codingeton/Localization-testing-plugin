{
  "manifest_version": 2,

  "name": "Localization Testing Plugin",
  "description": "This extension will Identify and highlight the English strings.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
        "content_scripts": [
          {
              "matches": ["file:///*/*","http://*/*","https://*/*"],
              "js": ["jquery.js","popup.js"]
        }  
        ]
}
